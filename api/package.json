{
  "name": "athena",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start ecosystem.config.js",
    "stop:prod": "pm2 stop Athena",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "cd .. && husky install",
    "migration:run": " yarn typeorm-ts-node-esm migration:run",
    "migration:create": "yarn typeorm-ts-node-esm migration:create -n {MIGRATION_NAME} src/migrations",
    "generate:migration": "yarn typeorm-ts-node-esm migration:generate src/migrations -d src/data-source.config.ts",
    "migration:revert": "yarn typeorm-ts-node-esm migration:revert",
    "db:refresh": "yarn typeorm-ts-node-esm schema:drop && migration:run"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/compression": "^1.7.2",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "4",
    "cache-manager-sqlite": "^0.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "eslint-plugin-import": "^2.27.5",
    "helmet": "^6.0.1",
    "html-to-text": "^9.0.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/webpack-preprocessor": "^5.17.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.0",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^9.0.0",
    "@types/jest": "27.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/pug": "^2.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cypress": "^12.8.1",
    "cypress-plugin-api": "^2.10.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.2.5",
    "lint-staged": "^13.1.0",
    "mssql": "^9.1.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.76.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,ts,}": [
      "yarn lint",
      "git add"
    ],
    "*.{js,ts,md}": [
      "yarn format",
      "git add"
    ]
  }
}
